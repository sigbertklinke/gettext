#: appinfo/app.php:23
msgid "Chat"
msgstr "Berriketa"

#: templates/main.php:57
msgid "{displayname} attached {path} to this conversation"
msgstr "{displayname} elkarrizketa honetara {path} atxikitu du"

#: templates/main.php:60
msgid "{displayname} removed {path} from this conversation"
msgstr "{displayname} elkarrizketa honetatik {path} ezabatu du"

#: templates/part.app-navigation.php:6
msgid "Start a chat with ..."
msgstr "Hasi txat bat honekin ..."

#: templates/part.app-navigation.php:12
msgid "Search in conversations"
msgstr "Bilatu berriketetan"

#: templates/part.avatar.php:163
msgid "Add Person"
msgstr "Gehitu pertsona"

